import java.io.FileOutputStream;
public class Madhav {
public static void main(String[] args) {
	String s= "I Love Krishna";
	byte b [] = s.getBytes();
	try {
	FileOutputStream sf = new FileOutputStream("D:\\Krishna.txt");
	sf.write(b[0]);
	}
	catch(Exception e){
		System.out.println("File not found");
	}
}
	
	
}
