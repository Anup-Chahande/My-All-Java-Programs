
public class arreyrevere {
public static void main(String[] args) {
	int temp;
	int a[] = {123,456,789};
	while(a[0]>0) {
     temp =a[0]%10;
     System.out.print(temp);
   a[0]=a[0]/10;
	
	}
	while(a[1]>0) {
	     temp =a[1]%10;
	     System.out.print(temp);
	   a[1]=a[1]/10;
	}
	   while(a[2]>0) {
		     temp =a[2]%10;
		     System.out.print(temp);
		   a[2]=a[2]/10;
		
}
}
